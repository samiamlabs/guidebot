<launch>

  <node pkg="move_base" type="move_base" respawn="false" name="move_base" output="screen" clear_params="true">
  <remap from="cmd_vel" to="nav_vel"/>
    <rosparam file="$(find guidebot_navigation)/config/base/costmap_common_params.yaml" command="load" ns="global_costmap" />
    <rosparam file="$(find guidebot_navigation)/config/base/costmap_common_params.yaml" command="load" ns="local_costmap" />
    <rosparam file="$(find guidebot_navigation)/config/base/local_costmap_params.yaml" command="load" />
    <rosparam file="$(find guidebot_navigation)/config/base/global_costmap_params.yaml" command="load" />
    <rosparam file="$(find guidebot_navigation)/config/base/base_local_planner_params.yaml" command="load" />
    <param name="controller_frequency" value="5.0" />
  </node>

</launch>
